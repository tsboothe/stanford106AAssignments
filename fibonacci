package week2;

import acm.program.*;

public class Fibonacci extends ConsoleProgram {
	
	
	public void run() {
		
		int fib1 = 0;
		int fib2 = 1;
		int fib3 = fib1 + fib2; 

		println("This Program lists the Fibonacci sequence");
		
		while (fib1 < MAX_TERM_VALUE) {
			println(fib1);
			fib2 = fib1;
			fib1 = fib3;
			fib3 = fib1 + fib2;
		}
		
	}
	private static final int MAX_TERM_VALUE = 10000;
}
